#بعض أنواع البيانات



print("hello world") #print 

print(type(100)) #int

print(type("hello")) #str

print(type({ "salah" : 1, "emad" : 2 })) #dict

print(type(191.23)) #float

print(type({ 1, 2, 3, 4, 6})) #set

print(type(( 1, 2, 3, 4, 5 ))) #tuple

print(type([ 1, 2, 3, 4])) #list

print(type(2 == 2)) #bool

